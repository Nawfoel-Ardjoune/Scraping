{
  "manifest_version": 3,
  "name": "My Linkedin script",
  "version": "2.0",
  "description": "Le script me permet d'accéder rapidement à des profiles",
  "permissions": [
    "activeTab",
    "storage",
    "cookies",
    "scripting",
    "downloads",
    "https://www.linkedin.com/"
  ],
  "host_permissions": [
    "https://www.linkedin.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/ia16.png",
      "48": "icons/ia48.png",
      "128": "icons/ia128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
  
}
